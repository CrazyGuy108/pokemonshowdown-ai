{
  "name": "pokemonshowdown-ai",
  "version": "0.0.1",
  "description": "desc",
  "main": "dist/index.js",
  "scripts": {
    "build:dex": "./build-dex.sh",
    "build": "tsc",
    "lint": "tslint -c tslint.json data/*.ts src/**/*.ts test/**/*.ts",
    "main": "ts-node src/index.ts",
    "test": "nyc npm run test:mocha",
    "test:mocha": "mocha",
    "train": "ts-node scripts/train.ts"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "coverage/",
      "scripts/",
      "src/data/",
      "dist/",
      "test/"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CrazyGuy108/pokemonshowdown-ai"
  },
  "keywords": [
    "pokemon"
  ],
  "author": "Taylor Hansen (CrazyGuy108)",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-node": "^0.1.17",
    "@types/websocket": "0.0.40",
    "readline": "^1.3.0",
    "websocket": "^1.0.28"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.5",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  }
}

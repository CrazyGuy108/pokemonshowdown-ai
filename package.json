{
  "name": "pokemonshowdown-ai",
  "version": "0.0.1",
  "description": "AI project for Pokemon Showdown",
  "private": true,
  "main": "dist/psbot/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "pretest": "tslint -c tslint.json -e src/battle/dex/dex.ts --project .",
    "psbot": "node dist/psbot/index.js",
    "test": "nyc mocha",
    "train": "node dist/train/index.js",
    "train:gpu": "npm run train -- --gpu"
  },
  "mocha": {
    "recursive": true,
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "spec": "test/**/*.test.ts"
  },
  "nyc": {
    "exclude": [
      "coverage/",
      "dist/",
      "pokemon-showdown/",
      "scripts/",
      "src/config.example.ts",
      "test/"
    ],
    "reporter": [
      "html",
      "lcov",
      "text"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorhansen/pokemonshowdown-ai"
  },
  "keywords": [
    "pokemon"
  ],
  "author": "Taylor Hansen (taylorhansen)",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs": "^2.6.0",
    "@tensorflow/tfjs-node": "^2.6.0",
    "@tensorflow/tfjs-node-gpu": "^2.6.0",
    "long": "^4.0.0",
    "node-fetch": "^2.6.1",
    "progress": "^2.0.3",
    "tfrecord": "^0.2.0",
    "tiny-typed-emitter": "^2.0.3",
    "tmp-promise": "^3.0.2",
    "websocket": "^1.0.32"
  },
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/long": "^4.0.1",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.10",
    "@types/node-fetch": "^2.5.7",
    "@types/progress": "^2.0.3",
    "@types/websocket": "^1.0.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "mocha": "^8.2.0",
    "nyc": "^15.1.0",
    "source-map-support": "^0.5.19",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3"
  }
}

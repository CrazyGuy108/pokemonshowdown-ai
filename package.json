{
  "name": "pokemonshowdown-ai",
  "version": "0.0.1",
  "description": "AI project for Pokemon Showdown",
  "private": true,
  "main": "dist/psbot/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "pretest": "tslint -c tslint.json -e src/battle/dex/dex.ts --project .",
    "psbot": "node dist/psbot/index.js",
    "test": "nyc mocha",
    "train": "node dist/train/index.js"
  },
  "mocha": {
    "recursive": true,
    "require": [
      "source-map-support/register",
      "ts-node/register"
    ],
    "spec": "test/**/*.test.ts"
  },
  "nyc": {
    "exclude": [
      "coverage/",
      "dist/",
      "pokemon-showdown/",
      "scripts/",
      "src/config.example.ts",
      "test/"
    ],
    "reporter": [
      "html",
      "lcov",
      "text"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorhansen/pokemonshowdown-ai"
  },
  "keywords": [
    "pokemon"
  ],
  "author": "Taylor Hansen (taylorhansen)",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-node": "^2.0.0",
    "node-fetch": "^2.6.0",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "^2.5.7",
    "@types/progress": "^2.0.3",
    "@types/websocket": "^1.0.1",
    "chai": "^4.1.2",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "progress": "^2.0.3",
    "source-map-support": "^0.5.19",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  }
}

{
  "name": "pokemonshowdown-ai",
  "version": "0.0.1",
  "description": "AI project for Pokemon Showdown",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "start": "node dist/index.js",
    "pretest": "tslint -c tslint.json -e src/battle/dex/dex.ts --project .",
    "test": "nyc mocha",
    "train": "ts-node scripts/train/index.ts"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "coverage/",
      "dist/",
      "pokemon-showdown/",
      "scripts/",
      "src/config.example.ts",
      "test/"
    ],
    "reporter": [
      "html",
      "lcov",
      "text"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/taylorhansen/pokemonshowdown-ai"
  },
  "keywords": [
    "pokemon"
  ],
  "author": "Taylor Hansen (taylorhansen)",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-data": "^1.5.1",
    "@tensorflow/tfjs-node": "^1.5.1",
    "node-fetch": "^2.6.0",
    "websocket": "^1.0.31"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node-fetch": "^2.5.4",
    "@types/websocket": "0.0.40",
    "chai": "^4.1.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}
